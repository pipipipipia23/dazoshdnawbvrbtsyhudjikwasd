local Client = require(game.ReplicatedStorage.Library.Client)
local Library = require(game.ReplicatedStorage.Library)
local Orbs = getsenv(game.Players.LocalPlayer.PlayerScripts.Scripts.Game.Orbs)
local aaaa = getsenv(game:GetService("Players").LocalPlayer.PlayerScripts.Scripts.GUIs.Teleport)
local Lootbags = getsenv(game.Players.LocalPlayer.PlayerScripts.Scripts.Game.Lootbags)
local v1 = require(game.ReplicatedStorage:WaitForChild("Framework"):WaitForChild("Library"))

getrenv().require = getgenv().require
getgenv().mapdefault = 'Axolotl Deep Ocean'
debug.setupvalue(Client.Network.Invoke, 1, function() return true end)
debug.setupvalue(Client.Network.Fire, 1, function() return true end)

workspace.__THINGS.Orbs.ChildAdded:Connect(function(v)
    Orbs.Collect(v)
end)

workspace.__THINGS.Lootbags.ChildAdded:Connect(function(v)
    for i,v in pairs(workspace.__THINGS.Lootbags:GetChildren()) do 
    if(v:GetAttribute("ReadyForCollection")) then
		if not v:GetAttribute("Collected") then
    			Lootbags.Collect(v)
		end
    end
end 
end)

function GetCoins(Area)
    local Coins = {}
    for i,v in next, Client.Network.Invoke('Get Coins') do 
        if v.a == Area then 
            Coins[i] = v
        end 
    end 
    return Coins
end
function DeleteCoin(Area)
    local Coins = {}
    for i,v in next, Client.Network.Invoke('Get Coins') do 
        if v.a ~= Area then 
            Coins[i] = v
        end 
    end 
    for i,v in next, Coins do
	wait()
 	if workspace.__THINGS.Coins:FindFirstChild(i) then
    		workspace.__THINGS.Coins:FindFirstChild(i):Destroy()
            
	end
    end
end
function DeleteMap(Area)
    for _,v in pairs(game:GetService("Workspace")["__MAP"].Areas:GetChildren()) do
	wait()
	if v.Name ~= Area then
		v:Destroy()
	end
    end
end
local eI = game:GetService("UserInputService")
local dp = game:GetService("RunService")



function teleportt(area)
	print(aaaa.Teleport(area,true))
end
function GetPets()
    return Client.PetCmds.GetEquipped() 
end 
function getboost()
local v52 = v1.Save.Get()
for _,v in pairs(v52.BoostsInventory) do
      if tonumber(v) ~= 0 then
             Client.Network.Fire("Activate Boost",1)
      end
end
end
function v422()
	for v9 = 1,12 do
		Client.Network.Invoke("Redeem Free Gift", v9)
	end
end
function lowsetting()
	settings().Rendering.QualityLevel = "Level01"
end
function lowfps()
	setfpscap(50)
end
function v273(v1,v2)
	return {
	    Recipient = tostring(v1),
	    Message = "Donate",
	    Pets = {},
            Diamonds = tonumber(v2)
	}
end

function Distancev2()
	if not game.workspace:FindFirstChild("Distance") then
		local a = Instance.new("Part", workspace)
		a.Name = "Distance"
		a.Anchored = true
		a.CanCollide = false
		a.CFrame = CFrame.new(256, 98.2, 351)
		a.Transparency = 1
	end
end

function v423(v1,v2)
	print(Client.Network.Invoke("Send Mail",v273(v1,v2)))
end

function v95(v1,v2)
	getgenv().account = tostring(v1)
	getgenv().soluong = tonumber(v2)
	v423(getgenv().account,getgenv().soluong)
end

local decalsyeeted = true
         local g = game
         local w = g.Workspace
         local l = g.Lighting
         local t = w.Terrain
         t.WaterWaveSize = 0
         t.WaterWaveSpeed = 0
         t.WaterReflectance = 0
         t.WaterTransparency = 0
         l.GlobalShadows = false
         l.FogEnd = 9e9
         l.Brightness = 0
         local success, result = pcall(lowsetting)

         for i, v in pairs(g:GetDescendants()) do
             if v:IsA("Part") or v:IsA("Union") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
                 v.Material = "Plastic"
                 v.Reflectance = 0
             elseif v:IsA("Decal") or v:IsA("Texture") and decalsyeeted then
                 v.Transparency = 1
             elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
                 v.Lifetime = NumberRange.new(0)
             elseif v:IsA("Explosion") then
                 v.BlastPressure = 1
                 v.BlastRadius = 1
             elseif v:IsA("Fire") or v:IsA("SpotLight") or v:IsA("Smoke") then
                 v.Enabled = false
             elseif v:IsA("MeshPart") then
                 v.Material = "Plastic"
                 v.Reflectance = 0
                 v.TextureID = 10385902758728957
             end
         end
         for i, e in pairs(l:GetChildren()) do
             if e:IsA("BlurEffect") or e:IsA("SunRaysEffect") or e:IsA("ColorCorrectionEffect") or e:IsA("BloomEffect") or e:IsA("DepthOfFieldEffect") then
                 e.Enabled = false
             end
         end
local players = game.Players.LocalPlayer.PlayerGui.Main.Boosts
local flag = true
local start = true
local p1 = Vector3.new(4169.8955078125, 9.766788482666016, 3796.091064453125)
local p2 = Vector3.new(9046, -14, 2411)
local DiamondMaxEarn = Library.Settings.DiamondMineMaxEarn
spawn(function()
while true do
    wait()
    if (p1-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude < 20 then
    if getgenv().mapdefault == "Axolotl Deep Ocean" then
	  local Pets = GetPets()
        local Coins = GetCoins('Axolotl Deep Ocean')
        local v42 = 1
        for i,v in next, Coins do
            if workspace.__THINGS.Coins:FindFirstChild(i) then
                      Client.Network.Invoke('Join Coin', i, {Pets[v42].uid})
                      Client.Network.Fire('Farm Coin', i, Pets[v42].uid)
                      v42 = v42 + 1
            end
            if v42 > #Pets then
            	v42 = 1
            end
        end 
    end
    end
    if (p2-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude < 20 then
    if getgenv().mapdefault == "Mystic Mine" then
        for i,v in pairs(GetCoins(getgenv().mapdefault)) do
		task.wait()
		if workspace.__THINGS.Coins:FindFirstChild(i) then
                for _,Pet in next, GetPets() do 
                    spawn(function()
                            Client.Network.Invoke('Join Coin',i, {Pet.uid})
                            Client.Network.Fire('Farm Coin', i, Pet.uid)
                    end)
	   	     end
	   	local currenttime = tick()
            repeat 
            task.wait()
            if tick() - currenttime > 120 then
            	break
            end
            until not workspace.__THINGS.Coins:FindFirstChild(i)
		end
        end 
        task.wait()
    end
    end
    task.wait()

end 
end)

