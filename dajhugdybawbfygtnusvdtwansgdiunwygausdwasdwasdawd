local DiamondMaxEarn = Library.Settings.DiamondMineMaxEarn
local demm = 0
local demm2 = 0
local afkcc = false
local checkfruit = true

spawn(function()
local success, result = pcall(b444)
while wait(5) do
    if checkfruit then
        
	demm = demm + 1
	if flag == false then
	for _,v in pairs(players:GetChildren()) do
        	if v.Name == "Orange" then
            	local asd = string.split(v.TimeLeft.Text, "x")[2]
            	if tonumber(asd) < 100 then
				flag = true
            	end
        	end
        end
	wait()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(9046, -14, 2411) 
    	--local v2 = v1.Directory.Areas[getgenv().mapdefault]
	--if v1.WorldCmds.Get() ~= v2.world then
	--teleportt(getgenv().mapdefault)
	--wait(5)
	--game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(9046, -14, 2411) 
	--else
	--game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(9046, -14, 2411) 
	--end
	if demm >= 10 then
	if Library.Save.Get().DiamondMineData.Earned >= DiamondMaxEarn then
		local playerDiamond = game:GetService("Players").LocalPlayer.leaderstats.Diamonds.Value
		if playerDiamond >= 50000200000 then
			if v1.WorldCmds.Get() == "Spawn" then
				Distancev2()
				wait(5)
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(255,98.2,348)
				wait(3)
				if game.workspace:FindFirstChild("Distance") then
					local distanceVector = game.workspace.Distance.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position
					local distanceMagnitude = distanceVector.magnitude
					if distanceMagnitude < 5 then
						print("thanhtuoi")
						v95("phamhiepgod",50000000000)
					end
				end
			end
		end
		wait(5)
		checkfruit = false
		demm = 0
		afkcc = true
	else
		if v1.WorldCmds.Get() == "Spawn" then
			teleportt(getgenv().mapdefault)
			wait(5)
		end
    	end
	end
  
	end
	if flag == true then
	getgenv().mapdefault = 'Axolotl Deep Ocean'
	local v2 = v1.Directory.Areas[getgenv().mapdefault]
	if v1.WorldCmds.Get() ~= v2.world then
	teleportt(getgenv().mapdefault)
	wait(5)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(4169.8955078125, 9.766788482666016, 3796.091064453125)
	else
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(4169.8955078125, 9.766788482666016, 3796.091064453125)  
	end
	for _,v in pairs(players:GetChildren()) do
        	if v.Name == "Orange" then
            	local asd = string.split(v.TimeLeft.Text, "x")[2]
            	if tonumber(asd) == 200 then
			getgenv().mapdefault = 'Mystic Mine'
			teleportt(getgenv().mapdefault)
			wait(5)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(9046, -14, 2411)
			flag = false

            	end
        	end
      end
	end
end
end
end)
spawn(function()
while wait(5) do
if afkcc then
	getgenv().mapdefault = 'Axolotl Deep Ocean'
	local v2 = v1.Directory.Areas[getgenv().mapdefault]
	if v1.WorldCmds.Get() ~= v2.world then
	teleportt(getgenv().mapdefault)
	wait(5)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(4169.8955078125, 9.766788482666016, 3796.091064453125)
	else
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(4169.8955078125, 9.766788482666016, 3796.091064453125)  
	end
	if demm2 >= 1440 then
		afkcc = false
		checkfruit = true
		flag = true
		demm2 = 0
	end
	demm2 = demm2 + 1

end
end
end)
